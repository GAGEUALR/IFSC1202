print("First Timestamp")

x = int(input("Enter Hours: "))
y = int(input("Enter Minutes: "))
z = int(input ("Enter Seconds: "))

a = int(x * 3600)
b = int(y * 60)
c = int(z)

print("Second Timestamp")

u = int(input("Enter Hours: "))
v = int(input("Enter Minutes: "))
w = int(input("Enter Seconds: "))

d = int(u * 3600)
e = int(v * 60)
f = int(w)

secinhours = int(d - a)
secinminutes = int(e - b)
secinsec = int(f - c)

print(secinhours - secinminutes - secinsec)